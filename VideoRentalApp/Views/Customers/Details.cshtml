@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>
    <h2>@Model.Name</h2>

    <p>
        <ul>
            <li>Membership Name: @Model.Membership.Name</li>
            @if (@Model.DateOfBirth != null)
            {
                <li>Birthday: @Model.DateOfBirth.ToString("dd-MMM-yyy")</li>
            }
        </ul>
    </p>

</body>

